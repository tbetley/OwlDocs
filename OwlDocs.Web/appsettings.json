{
  "ConnectionStrings": {
    "TestConnection": "Server=LENOVOX1;Database=OwlDocsTest;Trusted_Connection=True;Integrated Security=True;"
  },
  "DocumentProvider": "File",
  "DocumentProviderSettings": {
    "DirectoryRoot": "C:\\Documentation"
  },
  "AcceptedTextFileTypes": [
    ".md",
    ".txt"
  ],
  "AcceptedImageFileTypes": [
    ".png",
    ".gif",
    ".jpg",
    ".jpeg"
  ],
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": { "path": "C:\\Dev\\Logs\\OwlDocs\\log.txt" }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  }
}
